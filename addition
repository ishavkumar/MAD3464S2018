package day6;

/**
 *
 * @author macstudent
 */
public class Addition implements arithmetic, Multi {

    @Override
    public void display() {
        System.out.println("Addition : " + (n1 + n2));
    }

    public void multiplication() {
        System.out.println("Multiplication : " + (n1 * n2));

    }

}
